algoritmo max_entre_5;

variáveis x,y,k,z,w, max: real;
fim-variáveis
início
    imprima("Digite o primeiro valor");
    x:=leia();
    imprima("Digite o segundo valor");
    y:=leia();
    imprima("Digite o terceiro valor");
    k:=leia();
    imprima("Digite o quarto valor");
    z:=leia();
    imprima("Digite o quinto valor");
    w:=leia();

    max:=x;

   se x<y então
   max:=y;
   fim-se
   se y<k então
   max:=k;
   fim-se
   se k<z então
   max:=z;
   fim-se
   se z<w então
   max:=w;
   fim-se
   imprima("O maior valor é", max);
fim
